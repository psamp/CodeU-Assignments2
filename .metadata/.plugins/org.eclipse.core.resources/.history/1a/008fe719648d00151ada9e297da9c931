package com.psamp.auto;

import java.util.HashMap;
import java.util.Map;

public class AutoPolicy {
	double cost;
	Map<String, Integer> priceTiers = new HashMap<String, Integer>();
	
	private void populatePriceTiers() {
		priceTiers.put("Under 25", 1000);
		priceTiers.put("25-30", 800);
		priceTiers.put("31-40", 600);
		priceTiers.put("41-50", 400);
		priceTiers.put("50+", 200);
	}

	
	public AutoPolicy() {
		this.populatePriceTiers();
	}




	double getCost() {
		return this.cost;
	}

}
