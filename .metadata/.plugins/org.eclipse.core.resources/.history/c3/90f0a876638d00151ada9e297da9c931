package com.psamp.auto;

class Driver {
	int age;
	int drivingExperience;
	AutoPolicy policy;
	Gender gender;
	
	
	Driver(int age, String gender, int drivingExperience) {
		this.setAge(age);
		this.setDrivingExperience(drivingExperience);
		this.setGender(Gender.valueOf(gender));
	}
	
	int getAge() {
		return age;
	}
	private void setAge(int age) {
		this.age = age;
	}
	int getDrivingExperience() {
		return drivingExperience;
	}
	private void setDrivingExperience(int drivingExperience) {
		this.drivingExperience = drivingExperience;
	}
	AutoPolicy getPolicy() {
		return policy;
	}
	private void setPolicy(AutoPolicy policy) {
		this.policy = policy;
	}
	Gender getGender() {
		return gender;
	}
	void setGender(Gender gender) {
		this.gender = gender;
	}
	
	
	

}
